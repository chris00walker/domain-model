{
  "name": "elias-food-imports-domain-model",
  "version": "1.0.0",
  "description": "Domain model for Elias Food Imports using DDD principles",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cli": "tsx scripts/cli.ts",
    "gaps": "npm run cli -- gaps",
    "brief": "npm run cli -- brief",
    "matrix": "npm run cli -- matrix",
    "plantuml": "npm run cli -- plantuml",
    "enrich": "npm run cli -- enrich ./session-export.json",
    "precommit": "npm run lint && npm run test",
    "ddd": "efi-ddd all",
    "ci": "npm run ddd && npm test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "markdownlint-cli": "^0.32.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-monads": "^9.4.0"
  },
  "dependencies": {
    "uuid": "^11.1.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "simple-git": "^3.22.1",
    "slugify": "^1.6.6",
    "tsx": "^4.7.0",
    "unified": "^11.0.4"
  },
  "main": ".eslintrc.js",
  "bin": {
    "efi-ddd": "./scripts/cli.ts"
  },
